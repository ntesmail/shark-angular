    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序 - gulp",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "cwd": "${workspaceRoot}",
            "args": [
                "serve"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序 - app.js",
            "program": "${workspaceRoot}/app.js",
            "cwd": "${workspaceRoot}",
            "args": [
                "--port", "9003"
            ]
        },
        {
            "type": "node",
            "request": "attach",
            "name": "附加到进程",
            "port": 5858
        }
    ]
}
